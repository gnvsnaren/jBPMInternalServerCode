    <bpmn2:scriptTask id="_596566E2-DC79-493B-8D67-1515B7D45C1D" name="Check Bot Task" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Check Bot Task]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>_4C822FD7-34E8-4EE8-B4B4-5476F6E1651B</bpmn2:incoming>
      <bpmn2:script><![CDATA[try {
		System.out.println("processInstanceId = "+kcontext.getProcessInstance().getId());
		String s = null;
		String result_desc = null;
		String result_flag = null;
		
		com.InputParams inputParam = input.getInputparams();
		
		org.json.JSONObject inputToPython = new org.json.JSONObject();
		inputToPython.put("number", inputPayload.getNumber());
		inputToPython.put("description", new org.json.JSONObject(new com.google.gson.Gson().toJson(input.getInputparams())));
		inputToPython.put("processId", processId);
		inputToPython.put("sysId", inputPayload.getSys_id());
		System.out.println("This is the input to python: " + inputToPython);
		byte[] bytes = inputToPython.toString().getBytes();
		String inputToPythonBytes = javax.xml.bind.DatatypeConverter.printBase64Binary(bytes);
		System.out.println("This is the bytes: " + inputToPythonBytes);
		
		solutionIdToPythonMapping = solutionIdToPythonMapping.toString();
		System.out.println("solutionIdToPythonMapping: " + solutionIdToPythonMapping);
		org.json.JSONObject json = new org.json.JSONObject(solutionIdToPythonMapping);
		String pythonString = "python " + json.getString(input.getSolution_id()) + " " + inputToPythonBytes;
		System.out.println("This is the python script path" + pythonString);
		Process p = Runtime.getRuntime().exec(pythonString);
		
		java.io.BufferedReader in = new java.io.BufferedReader(new java.io.InputStreamReader(p.getInputStream()));
		java.io.BufferedReader stdError = new java.io.BufferedReader(new java.io.InputStreamReader(p.getErrorStream()));
		while ((s = in.readLine()) != null) {
			System.out.println(s);
		}
		System.out.println("Here is the standard error of the command (if any):\n");
        while ((s = stdError.readLine()) != null) {
            System.out.println(s);
        }
		String res_desc_path = "C://jbpm_project//SAP_OPs//Result//" + processId + "//" + processId + "_" + inputPayload.getSys_id() + "_" + input.getSolution_id() + "_res_desc.txt";
		String res_flag_path = "C://jbpm_project//SAP_OPs//Result//" + processId + "//" + processId + "_" + inputPayload.getSys_id() + "_" + input.getSolution_id() + "_res_flag.txt";
		java.io.File res_desc = new java.io.File(res_desc_path);
		java.util.Scanner descReader = new java.util.Scanner(res_desc);
		while (descReader.hasNextLine()) {
			result_desc = descReader.nextLine();
            System.out.println(result_desc);
		}
		descReader.close();
        java.io.File res_flag = new java.io.File(res_flag_path);
        java.util.Scanner flagReader = new java.util.Scanner(res_flag);
        while (flagReader.hasNextLine()) {
            result_flag = flagReader.nextLine();
            System.out.println(result_flag);
            kcontext.setVariable("checkBotFlag",result_flag);
			work_notes = "Solution Name - " + input.getSolution_id() + "\n" + "Solution Type - " + checkFlag + "\n" + "Result - " + result_desc;
			System.out.println("the work notes: " + work_notes);
			kcontext.setVariable("work_notes",work_notes);
			
			org.json.JSONObject executeOutcome = new org.json.JSONObject();
			executeOutcome.put("solution_id", input.getSolution_id() + "");
			executeOutcome.put("exec_status", "true");
			executeOutcome.put("input_params", new com.google.gson.Gson().toJson(input.getInputparams()));
			executeOutcome.put("exec_outcome", work_notes);
			
			java.util.List<org.json.JSONObject> outcome_list = new java.util.ArrayList<org.json.JSONObject>();
			java.lang.Object object = kcontext.getVariable("execution_outcome_array");
			if(object != null){
				String exec_output = object.toString().substring( 1, object.toString().length() - 1 );
				System.out.println("This is the string json: " + exec_output);
				org.json.JSONObject obj = new org.json.JSONObject(exec_output);
				System.out.println("This is the json object: " + obj);
				outcome_list = new java.util.ArrayList(java.util.Arrays.asList(exec_output));
				System.out.println("this is after as list: " + outcome_list);
			}
			
			outcome_list.add(executeOutcome);
			System.out.println("The outcome list in check bot: " + outcome_list);
			kcontext.setVariable("execution_outcome_array",outcome_list.toString());
        }
        flagReader.close();
	} 
catch (Throwable th) {
	th.printStackTrace();
}]]></bpmn2:script>
