          </drools:metaData>
          <drools:onEntry-script scriptFormat="http://www.java.com/java">
            <drools:script><![CDATA[System.out.println("Inside Checkbot Rest API");
org.json.JSONObject soapJsonObj = new org.json.JSONObject();
org.json.JSONObject variable = new org.json.JSONObject();
com.capgemini.sapopsresolver.InputParams inputParam = input.getInput_params();
String[] param1 = inputParam.getParam1().split("[:]");
try{
	System.out.println("This is the input parameter: " + param1[1]);
	variable.put(param1[0], param1[1]);
}catch(java.lang.ArrayIndexOutOfBoundsException e){
	variable.put(param1[0], "null");
}
org.json.JSONObject bot = new org.json.JSONObject();
String botString = "urn:" + input.getSolution_id();
bot.put(botString, variable);
soapJsonObj.put("soapenv:Body", bot);
System.out.println("This is in java code of json:: " + soapJsonObj.toString());

String xml = org.json.XML.toString(soapJsonObj);
System.out.println("This is the xml: " + xml);
String soapenv = "http://schemas.xmlsoap.org/soap/envelope/";
String urn = "urn:sap-com:document:sap:rfc:functions";
String soapInput = "<soapenv:Envelope xmlns:soapenv='" + soapenv +"' xmlns:urn='" + urn + "'>" + xml + "</soapenv:Envelope>";
System.out.println("This is the soap xml: " + soapInput);
kcontext.setVariable("restinput",soapInput);
kcontext.setVariable("checkUrl",input.getWebservice_url());]]></drools:script>
          </drools:onEntry-script>
          <drools:onExit-script scriptFormat="http://www.java.com/java">
            <drools:script><![CDATA[org.json.JSONObject json = org.json.XML.toJSONObject(restoutput);
System.out.println("This is in output of check bot: " + json.toString());
org.json.JSONObject Envelope = json.getJSONObject("soap-env:Envelope");
org.json.JSONObject Body = Envelope.getJSONObject("soap-env:Body");
String botString = "n0:" + input.getSolution_id() + "Response";
org.json.JSONObject Response = Body.getJSONObject(botString);
String E_MSGFLAG = Response.getString("E_MSGFLAG");
String E_MESSAGE = Response.getString("E_MESSAGE");
System.out.println("This is the flag: " + E_MSGFLAG);
//work_notes = kcontext.getVariable("work_notes") + "" + checkflag + " bot: " + input.getSolution_id() + " \n " + "Result: " + E_MESSAGE + " \n ";

work_notes = "Solution Name - " + input.getSolution_id() + "\n" + "Solution Type - " + checkflag + "\n" + "Result - " + E_MESSAGE;
System.out.println("the work notes: " + work_notes);
kcontext.setVariable("work_notes",work_notes);
kcontext.setVariable("checkbotflag",E_MSGFLAG);
if(E_MSGFLAG.equals("F") && input.getAllowresolve() == false) {
	kcontext.setVariable("checkbotflag","S");
	kcontext.setVariable("botResolve",0);
}

org.json.JSONObject executeOutcome = new org.json.JSONObject();
executeOutcome.put("solution_id", input.getSolution_id());
executeOutcome.put("exec_status", "true");
executeOutcome.put("input_params", new com.google.gson.Gson().toJson(input.getInput_params()));
executeOutcome.put("exec_outcome", work_notes);

//String str = kcontext.getVariable("execution_outcome_array");
//java.util.List outcome_list = (java.util.List) kcontext.getVariable("execution_outcome_array");
//if(outcome_list == null) outcome_list = new java.util.ArrayList();

java.util.List<org.json.JSONObject> outcome_list = new java.util.ArrayList<org.json.JSONObject>();
java.lang.Object object = kcontext.getVariable("execution_outcome_array");
if(object != null){
	String exec_output = object.toString().substring( 1, object.toString().length() - 1 );
	System.out.println("This is the string json: " + exec_output);
	org.json.JSONObject obj = new org.json.JSONObject(exec_output);
	System.out.println("This is the json object: " + obj);
	outcome_list = new java.util.ArrayList(java.util.Arrays.asList(exec_output));
	System.out.println("this is after as list: " + outcome_list);
}

outcome_list.add(executeOutcome);
System.out.println("The outcome list in check bot: " + outcome_list);
kcontext.setVariable("execution_outcome_array",outcome_list.toString());]]></drools:script>
          </drools:onExit-script>
