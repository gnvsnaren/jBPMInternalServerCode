        <bpmn2:script><![CDATA[System.out.println("This is inside resolve disk space task");
String inputseq = input.getSolution_seq_no();
for (com.Solutions solution : inputPayload.getSolutions()){
	String solutionseq = solution.getSolution_seq_no();
    String CheckFlag=solution.getCheckresolveflag();
    if(solutionseq.equals(inputseq) && CheckFlag.equals("Resolve")){
		System.out.println("This is inside ResolveDiskSpaceTask");
		try {
				String s = null;
				Process p = Runtime.getRuntime().exec("python C://jbpm_project//solutions//Free_Disk_Space//Free_Disk_Space//Main.py");
				java.io.BufferedReader in = new java.io.BufferedReader(new java.io.InputStreamReader(p.getInputStream()));
				java.io.BufferedReader stdError = new java.io.BufferedReader(new java.io.InputStreamReader(p.getErrorStream()));
				while ((s = in.readLine()) != null) {
					System.out.println(s);
				}
				System.out.println("Here is the standard error of the command (if any):\n");
				while ((s = stdError.readLine()) != null) {
					System.out.println(s);
				}
				
				String resolveAppName = solution.getCheckresolveflag();
				String resolveSolutionName = solution.getSolution_id();
				com.InputParams resolveInputParams = solution.getInput_params();
				String E_MESSAGE = "Disk Space Cleared";
				
				work_notes = "Solution Name - " + resolveSolutionName + "\n" + "Solution Type - " + resolveAppName + "\n" + "Result - " + E_MESSAGE;
				kcontext.setVariable("work_notes",work_notes);
				System.out.println("The work notes in resolve bot: " + work_notes);

				org.json.JSONObject executeOutcome = new org.json.JSONObject();
				executeOutcome.put("solution_id", resolveSolutionName);
				executeOutcome.put("exec_status", "true");
				executeOutcome.put("input_params", new com.google.gson.Gson().toJson(resolveInputParams));
				executeOutcome.put("exec_outcome", work_notes);
				
				//System.out.println("The execution_outcome_array in resolve bot: " + execution_outcome_array);
				
				java.util.ArrayList outcome_list = new java.util.ArrayList();     
				if (execution_outcome_array != null){
					org.json.JSONObject jsonObject = new org.json.JSONObject(execution_outcome_array);
					org.json.JSONArray jArray = new org.json.JSONArray(jsonObject);
					if (jArray != null) { 
						for (int i=0;i<jArray.length();i++){ 
							outcome_list.add(jArray.getString(i));
						} 
					}
				}
				if(outcome_list == null) outcome_list = new java.util.ArrayList();
				outcome_list.add(executeOutcome);
				System.out.println("The outcome list in resolve bot: " + outcome_list);
				kcontext.setVariable("execution_outcome_array",outcome_list.toString());
			} 
		catch (Throwable th) {
			th.printStackTrace();
		}
	}
}]]></bpmn2:script>
